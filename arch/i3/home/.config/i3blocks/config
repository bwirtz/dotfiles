interval=2

[separator]

[memory]
command=$SCRIPT_DIR/scripts/memory.py
interval=20

[separator]

[cpu]
command=$SCRIPT_DIR/scripts/cpu.py
min_width="0 xxxxx 100%"
interval=1

[separator]

[battery]
command=$SCRIPT_DIR/scripts/battery.py
interval=5

[separator]

[wifi]
instance=wlp10s0
